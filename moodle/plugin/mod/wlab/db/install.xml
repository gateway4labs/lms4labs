<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/wlab/db" VERSION="20110919" COMMENT="XMLDB file for Moodle mod/wlab"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>

    <TABLE NAME="wlab_access" COMMENT="wlabs information of access" NEXT="wlab_list">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of type access to wlab" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="name of weblab" PREVIOUS="id" NEXT="version"/>
        <FIELD NAME="version" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="version of weblab" PREVIOUS="name"/>
	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for wlab"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="wlab_list" COMMENT="labs information" PREVIOUS="wlab_access" NEXT="wlab_experiment">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="name of weblab" PREVIOUS="id" NEXT="description"/>
	  <FIELD NAME="description" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" COMMENT="description of weblab" PREVIOUS="name" NEXT="accesstype"/>
	  <FIELD NAME="accesstype" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="moodle user of weblab" PREVIOUS="description"/>
	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for labs" NEXT="access"/>
        <KEY NAME="access" TYPE="foreign" FIELDS="accesstype" REFTABLE="wlab_access" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="wlab_experiment" COMMENT="experiments that is possible to carry out" PREVIOUS="wlab_list" NEXT="wlab_experiment_course">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idlab"/>
        <FIELD NAME="idlab" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of laboratory" PREVIOUS="id" NEXT="name"/>
	  <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="description experiment of weblab" PREVIOUS="idlab" NEXT="uniqueid"/>
	  <FIELD NAME="uniqueid" TYPE="char" LENGTH="100" NOTNULL="false" ENUM="false" COMMENT="data1 of weblab" PREVIOUS="name"/>
  	 </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for wlab" NEXT="idlab"/>
        <KEY NAME="idlab" TYPE="foreign" FIELDS="idlab" REFTABLE="wlab_list" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="wlab_experiment_course" COMMENT="experiments that is possible to carry out" PREVIOUS="wlab_experiment" NEXT="wlab">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idexp"/>
        <FIELD NAME="idexp" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id of experiment" PREVIOUS="id" NEXT="course"/>
  	  <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="idexp"/> 
	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for experiment" NEXT="idexp"/>
        <KEY NAME="idexp" TYPE="foreign" FIELDS="idexp" REFTABLE="wlab_experiment" REFFIELDS="id" PREVIOUS="primary" NEXT="course"/>
        <KEY NAME="course" TYPE="foreign" FIELDS="course" REFTABLE="course" REFFIELDS="id" PREVIOUS="idexp"/>
      </KEYS>
    </TABLE>
   

    <TABLE NAME="wlab" COMMENT="information course and labs" PREVIOUS="wlab_experiment_course" NEXT="wlab_connection_deusto">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idexp"/>
        <FIELD NAME="idexp" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id of experiment" PREVIOUS="id" NEXT="course"/>
	  <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="idexp" NEXT="name"/> 
	  <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="name of weblab" PREVIOUS="course"/> 
	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for wlab" NEXT="idexp"/>
        <KEY NAME="idexp" TYPE="foreign" FIELDS="idexp" REFTABLE="wlab_experiment" REFFIELDS="id" PREVIOUS="primary" NEXT="course"/>
        <KEY NAME="course" TYPE="foreign" FIELDS="course" REFTABLE="course" REFFIELDS="id" PREVIOUS="idexp"/>
      </KEYS>
    </TABLE>


    <TABLE NAME="wlab_connection_deusto" COMMENT="wlabs information" PREVIOUS="wlab" NEXT="wlab_connection_ilab">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idlab"/>
        <FIELD NAME="idlab" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id of laboratory" PREVIOUS="id" NEXT="username"/>
	  <FIELD NAME="username" TYPE="char" LENGTH="255" NOTNULL="true" PREVIOUS="idlab" NEXT="pass"/>
        <FIELD NAME="pass" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="moodle pass of weblab" PREVIOUS="username" NEXT="url"/>
 	  <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="url of weblab" PREVIOUS="pass"/>
 	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for wlab" NEXT="idlab"/>
        <KEY NAME="idlab" TYPE="foreign" FIELDS="idlab" REFTABLE="wlab_list" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>

    <TABLE NAME="wlab_connection_ilab" COMMENT="wlabs information" PREVIOUS="wlab_connection_deusto" NEXT="wlab_experiment_user_deusto">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idlab"/>
        <FIELD NAME="idlab" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id of laboratory" PREVIOUS="id" NEXT="username"/>
	  <FIELD NAME="username" TYPE="char" LENGTH="255" NOTNULL="true" PREVIOUS="idlab" PREVIOUS="idlab" NEXT="pass"/>
        <FIELD NAME="pass" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="moodle pass of weblab" PREVIOUS="username" NEXT="url"/>
 	  <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="url of weblab" PREVIOUS="pass"/>
 	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for wlab" NEXT="idlab"/>
        <KEY NAME="idlab" TYPE="foreign" FIELDS="idlab" REFTABLE="wlab_list" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
   
    <TABLE NAME="wlab_experiment_user_deusto" COMMENT="experiments that is possible to carry out" PREVIOUS="wlab_connection_ilab" NEXT="wlab_experiment_user_ilab">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idexp"/>
        <FIELD NAME="idexp" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id of experiment" PREVIOUS="id" NEXT="course"/>
  	  <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="idexp" NEXT="sessionid"/>  
 	  <FIELD NAME="sessionid" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="sessionid of weblab" PREVIOUS="course"/>
	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for wlab" NEXT="idexp"/>
        <KEY NAME="idexp" TYPE="foreign" FIELDS="idexp" REFTABLE="wlab_experiment" REFFIELDS="id" PREVIOUS="primary" NEXT="course"/>
        <KEY NAME="course" TYPE="foreign" FIELDS="course" REFTABLE="course" REFFIELDS="id" PREVIOUS="idexp"/>
      </KEYS>
    </TABLE>

<TABLE NAME="wlab_experiment_user_ilab" COMMENT="experiments that is possible to carry out" PREVIOUS="wlab_experiment_user_deusto">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of wlab" NEXT="idexp"/>
        <FIELD NAME="idexp" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="id of experiment" PREVIOUS="id" NEXT="course"/>
