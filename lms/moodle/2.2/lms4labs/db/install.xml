<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/wlab/db" VERSION="20110919" COMMENT="XMLDB file for Moodle mod/wlab"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  
  <TABLES>

    <TABLE NAME="lms4labs" COMMENT="lms4labs information of access">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id" NEXT="name"/>
	<FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="name" PREVIOUS="id" NEXT="value"/>
        <FIELD NAME="value" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="value of key" PREVIOUS="name"/>
	</FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for lms4labs"/>
      </KEYS>
    </TABLE>

   

  </TABLES>
</XMLDB>